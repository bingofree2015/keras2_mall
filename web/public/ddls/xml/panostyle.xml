<krpano>
	

	<!-- 热点图标 -->
	
	


						   
	<style name="musiclayer" 
		keep="true" 
		align="lefttop"
		scale="0.5"
	/>
				
	<style name="hotspot_tuwen_style" 
		fillcolor="0xaaaaaa" 
		fillalpha="0" 
		borderwidth="3.0" 
		onout="tween(hotspot[get(name).borderalpha,0,0.1]);" 
		onhover="tween(hotspot[get(name).borderalpha,0.8]);" 
		bordercolor="0x9C01CB" 
		borderalpha="0"
		depth="1000"
		polyline="false"
		scaleflying="true"
		vcheck="0"
		tx="0"
		ty="0"
		tz="0"
	/>
	
	<style name="hotspot_point" 
		url="%SWFPATH%/skin/note.png"
		scale="1"
		onloaded=""
		distorted="false"
		zoom="true"
		normal="false"
	/>
	
	<style name="hotspot_point1" 
		scale="0.5"
	    url="%SWFPATH%/skin/note1.png"
		distorted="true"
		vcheck="0"
	/>
	
	<style name="hotspot_point2" 
		scale="0.5"
	    url="%SWFPATH%/skin/vedio.png"
		distorted="true"
		vcheck="0"
	/>
	
	<style name="spotsb"  scale="0.5"  distorted="false" zoom="true" keep="false"  vcenter="true"  hcenter="true" 	
	       tooltip=""  blendmode="normal"  alpha="0.8"  scroll="1" direction="1"
		   smoothing="true" pixelhittest="false" normal="false"
	       onclick=""
	       onloaded="hotspot_animate();"
		   url="%SWFPATH%/skin_M_kr/spot.png"
	       crop="0|0|58|58" 
		   framewidth="58" 
		   frameheight="58"
	       frames="16"  
		   frame="1"
	       />
	<style name="spotouter" scale="0.2" distorted="false" zoom="true" keep="true" vcenter="true"  hcenter="true" 
	       tooltip=""  blendmode="normal"  alpha="1"  scroll="1" direction="1"
		   smoothing="true" pixelhittest="false" normal="false"
	       onclick=""
	       onloaded=""
		   url="%SWFPATH%/skin_M_kr/spotww.png"	       
	       />	   

	<style name="hotspotaa" scale="0.5" edge="top" oy="0" distorted="false" zoom="true" keep="false"  vcenter="true"	
	       tooltip=""  		autoalpha="false" blendmode="normal"  alpha="0.8" musiccheck="true" scroll="1" direction="1" picsss="" title=""
		   effect="" smoothing="true" pixelhittest="false" normal="false"
	       onclick=""
	       onloaded="hotspot_animate();"
		   url="%SWFPATH%/skin_M_kr/spot.png"
	       crop="0|0|58|58" 
		   framewidth="58" 
		   frameheight="58"
	       frames="16"  
		   frame="1"
	       />

<!-- coordinatex.xml 里面的style-->

<!-- 正常白点坐标   全部显示小 -->
	<style name="floorspot" keep="true" type="text" url="%SWFPATH%/skin_M_kr/spotss.png" 
			vcenter="true"
			width="10" height="10" scale="0.015"
			visible="false"
			oversampling="2" mipmapping="true" zorder="1"
			distorted="true" depth="0" depthbuffer="true"
			torigin="world" rotationorder="xyz" rx="-90"
			xalpha="0"
			alpha="0.2"
			onover.addevent="tween(scale,0.015); tween(alpha,0.7);"
			onout.addevent="tween(scale,0.015); tween(alpha,0.2);"
			onclick=""
			/>
<!-- 呼出模型显示的单元图标 全部显示 -->
	<style name="floorspot2" keep="true" type="text" url="" 
			vcenter="true"
			visible="false"
			width="10" height="10" scale="4"
			oversampling="2" mipmapping="true" zorder="1"
			distorted="false" depth="0" depthbuffer="true"
			torigin="world" rotationorder="xyz" rx="-90"
			xalpha="0"
			alpha="1"
			onclick=""
			/>	
<!--正常坐标  只当前场景显示 加在tour.xml里面 -->
	<style name="floorspot3" keep="false"  type="text" url="%SWFPATH%/skin_M_kr/spotss.png" 
			vcenter="true"
			width="10" height="10" scale="0.015"
			visible="false"
			oversampling="2" mipmapping="true" zorder="1"
			distorted="true" depth="0" depthbuffer="true"
			torigin="world" rotationorder="xyz" rx="-90"
			xalpha="0"
			alpha="0.2"
			onover.addevent="tween(scale,0.015); tween(alpha,0.5);"
			onout.addevent="tween(scale,0.015); tween(alpha,0.8);"
			onclick=""
			/>
<!-- 呼出模型显示的隐藏图标  点击任何位置都可进入场景 capture="false"   不捕获拖动事件 -->
	<style name="floorspot4" keep="true" type="text" url="%SWFPATH%/skin_M_kr/aaaaa.png" 
			vcenter="true" 
			visible="false"
			width="10" height="10" scale="0.2"
			oversampling="2" mipmapping="true" zorder="1"
			distorted="true" depth="0" depthbuffer="true"
			torigin="world" rotationorder="xyz" rx="-90"
			capture="false" 
			 maskchildren="true" 
			ondown=""
			xalpha="0"
			alpha="1"
			onclick=""
			/>
<!-- 呼出模型显示的当前位置图标  -->	
<style name="floorspot5" keep="true" type="image" url="%SWFPATH%/skin_M_kr/settop.png" 
			vcenter="true"
			visible="false" 
			width="10" height="10" scale="0.8"
			oversampling="2" mipmapping="true" zorder="1"
			distorted="false" depth="0" depthbuffer="true"
			torigin="world" rotationorder="xyz" rx="-90"
			xalpha="0"
			alpha="1"
			onclick=""
			/>	

<!-- icon.xml 里面的style -->
<style name="icongo" keep="true" type="text" url="%SWFPATH%/skin_M_kr/icongo.png"
		vcenter="true"
		width="10" height="10" scale="0.01"		
		oversampling="2" mipmapping="true" zorder="1"
		distorted="true" depth="0" depthbuffer="true"
		torigin="world" rotationorder="xyz" rx="-90"
		xalpha="0"
		alpha="0.2"
		enabled="false"
		/>			


</krpano>
